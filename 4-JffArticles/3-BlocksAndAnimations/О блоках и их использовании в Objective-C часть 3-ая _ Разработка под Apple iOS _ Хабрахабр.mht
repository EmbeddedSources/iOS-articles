    pre .attr_selector,
    pre .pseudo,
    pre .literal,
    pre .keyword,
    pre .hexcolor,
    pre .css .function,
    pre .ini .title,
    pre .css .class,
    pre .list .title,
    pre .tex .command {
      color: #4d7386;
    }

    pre .attribute,
    pre .rules .keyword,
    pre .number,
    pre .date,
    pre .regexp,
    pre .tex .special {
      color: #2f98ff;
    }

    pre .number,
    pre .regexp {
      font-weight: normal;
    }

    pre .string,
    pre .value,
    pre .filter .argument,
    pre .css .function .params,
    pre .apache .tag {
